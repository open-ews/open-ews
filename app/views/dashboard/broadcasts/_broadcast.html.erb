<%= content_tag_for(:tr, broadcast, role: "row") do %>
  <td>
    <%= link_to(broadcast.id, dashboard_broadcast_path(broadcast)) %>
  </td>
  <td>
    <%= broadcast.channel.humanize %>
  </td>
  <td>
    <%= broadcast_status(broadcast) %>
  </td>
  <td>
    <%= local_time(broadcast.created_at) %>
  </td>
<% end %>
