= page_title(title: title) do
  = render("shared/edit_resource_page_action", path: edit_dashboard_access_token_path(resource))
  = render("shared/destroy_resource_page_action", path: dashboard_access_token_path(resource))

.card-body
  = show_for(resource) do |f|
    = f.attribute :id
    = f.attribute :token
    = f.attribute :permissions, value: resource.permissions_text
    = f.attribute :created_at, value: local_time(resource.created_at)
