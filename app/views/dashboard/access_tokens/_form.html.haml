.card-body
  = simple_form_for([:dashboard, resource]) do |f|
    = f.input :permissions, collection: AccessToken.values_for_permissions, as: :check_boxes, checked: resource.permissions
    .form-group.actions
      = f.button :submit, class: 'btn btn-primary'
      = link_to(translate(:"titles.actions.cancel"), :back, class: 'btn btn-outline-dark')
