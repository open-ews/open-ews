= content_tag_for(:tr, phone_call) do
  %td
    = link_to(phone_call.id, show_location(phone_call))
  %td
    = phone_call.phone_number
  %td
    = phone_call.direction.to_s.humanize
  %td
    = phone_call.status.humanize
  %td
    = phone_call.duration
  %td
    = local_time(phone_call.created_at)
