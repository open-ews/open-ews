= content_for(:page_actions) do
  = render("shared/new_resource_page_action", path: new_dashboard_beneficiary_path)

.card
  .card-header
    .card-actions
      %a.btn
        %i.icon.ti.ti-filter
        Filter
      %a.btn
        %i.icon.ti.ti-file-export
        Export
      = render("shared/import_action", import_params: { resource_type: "Beneficiary" }, title: "Import Beneficiaries" )

  = render("shared/resources_table", resource_class: Beneficiary, columns: [:phone_number, :gender, :created_at])
  = render("shared/pagination", resources: resources)
